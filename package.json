{
  "name": "alxhghs",
  "private": true,
  "description": "Personal website for Alex Hughes",
  "version": "1.0.0",
  "author": "Alex Hughes <alxhghs@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "format": "npx prettier --write .",
    "start": "next start",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.tsx": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "dayjs": "^1.10.7",
    "next": "^12.0.1",
    "react": "^18.0.0-alpha-9c8161ba8-20211028",
    "react-dom": "^18.0.0-alpha-9c8161ba8-20211028"
  },
  "devDependencies": {
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  }
}
